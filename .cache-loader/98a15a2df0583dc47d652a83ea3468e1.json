2C;;AAAA,SAoB5CE,MApB4C,GAoBlCC,GAAD,IAAyB;AAC9B,WAAKV,YAAL,GAAoBU,GAApB;AACA,WAAKL,OAAL;AACH,KAvB2C;;AAAA,SAyB5CM,kBAzB4C,GAyBtBC,IAAD,IAA6B;AAC9C,UAAMC,QAAQ,GAAGD,IAAI,KAAK,MAAT,GAAkBE,MAAM,CAACC,gBAAzB,GAA4CD,MAAM,CAACE,mBAApE;AACAH,MAAAA,QAAQ,CAAC,QAAD,EAAW,KAAKX,YAAhB,EAA8B,KAA9B,CAAR;AACH,KA5B2C;AAAA;;AA8B5Ce,EAAAA,iBAAiB,GAAG;AAChB,SAAKN,kBAAL,CAAwB,MAAxB;AACH;;AAEDO,EAAAA,oBAAoB,GAAG;AACnB,SAAKP,kBAAL,CAAwB,QAAxB;AACH;;AAEDQ,EAAAA,MAAM,GAAG;AAAA,sBACkC,KAAKC,KADvC;AAAA,QACGC,SADH,eACGA,SADH;AAAA,QACcC,KADd,eACcA,KADd;AAAA,QACqBC,QADrB,eACqBA,QADrB;AAAA,sBAEqB,KAAK1B,KAF1B;AAAA,QAEGE,KAFH,eAEGA,KAFH;AAAA,QAEUD,MAFV,eAEUA,MAFV;AAGL,WACI;AAAK,MAAA,SAAS,EAAEuB,SAAhB;AAA2B,MAAA,KAAK,EAAEC,KAAlC;AAAyC,MAAA,GAAG,EAAE,KAAKb;AAAnD,OACKc,QAAQ,CAAC;AAAExB,MAAAA,KAAF;AAASD,MAAAA;AAAT,KAAD,CADb,CADJ;AAKH;;AA9C2C;;AAiDhD,eAAeH,SAAf","sourcesContent":["import * as React from 'react'\n\ninterface Size {\n    width: number\n    height: number\n}\n\ninterface IP {\n    className?: string\n    style?: React.CSSProperties\n    children?: (props: Size) => React.ReactNode\n}\n\ninterface IS {\n    height: number\n    width: number\n}\n\nclass AutoSizer extends React.Component<IP, IS> {\n    state = { height: 0, width: 0 }\n\n    private containerRef: HTMLDivElement = null\n    private timer: NodeJS.Timer = null\n\n    listenResize = () => {\n        clearTimeout(this.timer)\n        this.timer = setTimeout(() => {\n            this.setSize()\n        }, 100)\n    }\n\n    setSize = () => {\n        if (this.containerRef) {\n            const { clientHeight, clientWidth } = this.containerRef\n            this.setState({ height: clientHeight, width: clientWidth })\n        }\n    }\n\n    setRef = (ref: HTMLDivElement) => {\n        this.containerRef = ref\n        this.setSize()\n    }\n\n    bindOrUnbindResize = (type: 'bind' | 'unbind') => {\n        const listener = type === 'bind' ? window.addEventListener : window.removeEventListener\n        listener('resize', this.listenResize, false)\n    }\n\n    componentDidMount() {\n        this.bindOrUnbindResize('bind')\n    }\n\n    componentWillUnmount() {\n        this.bindOrUnbindResize('unbind')\n    }\n\n    render() {\n        const { className, style, children } = this.props\n        const { width, height } = this.state\n        return (\n            <div className={className} style={style} ref={this.setRef}>\n                {children({ width, height })}\n            </div>\n        )\n    }\n}\n\nexport default AutoSizer\n"]}]}